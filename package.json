{
  "name": "notion-boost-browser-extension",
  "version": "1.0.0",
  "description": "Notion Boost - add outline and more to notion.so",
  "private": false,
  "main": "index.js",
  "scripts": {
    "start:ch": "webpack --watch --mode=development --browser=chrome",
    "start:ff": "webpack --watch --mode=development --browser=firefox",
    "build:ch": "webpack --mode=production --browser=chrome",
    "build:ff": "webpack --mode=production --browser=firefox",
    "//mode": "https://webpack.js.org/configuration/mode/",
    "prebuild:ff": "git diff-index --quiet HEAD -- || echo 'WARNING: commit changes before submitting firefox addon'",
    "postbuild:ff": "curl -L  http://github.com/GorvGoyl/Notion-Boost-browser-extension/archive/master.zip --output build_firefox/master.zip",
    "lint": "eslint ./src/ -c .eslintrc.json --ext .js",
    "lint-fix": "eslint ./src/ -c .eslintrc.json --ext .js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.2.0",
    "html-webpack-plugin": "^4.3.0",
    "merge-jsons-webpack-plugin": "^1.0.21",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "preact": "^10.5.2",
    "preact-router": "^3.2.1"
  }
}
